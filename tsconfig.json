{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "strict": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "target": "es5",
        "lib" : ["es2015", "es2015.iterable", "dom"]
    },
    "include": [
        "./src/**/*"
    ]
}
